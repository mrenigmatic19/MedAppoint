        <li class="list-group-item k card border-warning">Equipments Name</li>
        <input type="text" class="form-control card border-primary" name="instrumentname">
      </ul>
      <ul class="list-group list-group-horizontal k1">
        <li class="list-group-item k card border-warning">Type</li>
        <input type="text" class="form-control card border-primary" name="type">
      </ul>
      <ul class="list-group list-group-horizontal k1">
        <li class="list-group-item k card border-warning">Availability</li>
        <input type="text" class="form-control card border-primary" name="availability">
      </ul>
     
      
        <input type="submit" name="submit" class="btn btn-primary" value="Save">

    </div>
        </form>
  <div class="container">
